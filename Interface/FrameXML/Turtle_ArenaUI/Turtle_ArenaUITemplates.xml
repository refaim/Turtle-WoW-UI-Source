                    </Anchor>
                </Anchors>
                <Backdrop edgeFile="Interface\ArenaFrame\UI-Character-PVP-Highlight" tile="true">
                    <EdgeSize>
                        <AbsValue val="32" />
                    </EdgeSize>
                    <BorderColor r="1" g="0.82" b="0" a="0.7" />
                </Backdrop>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad>
                this:SetAlpha(0.4)
            </OnLoad>
            <OnClick>
                ArenaTeam_OnClick()
            </OnClick>
            <OnEnter>
                getglobal(this:GetName() .. "Highlight"):Show()
            </OnEnter>
            <OnLeave>
                getglobal(this:GetName() .. "Highlight"):Hide()
            </OnLeave>
        </Scripts>
