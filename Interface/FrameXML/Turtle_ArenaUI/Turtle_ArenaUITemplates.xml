                        <Scripts>
                            <OnClick>
                                ArenaFrameDetailsDisbandButton_OnClick()
                            </OnClick>
                            <OnEnter>
                                GameTooltip:SetOwner(this, "ANCHOR_RIGHT")
                                GameTooltip:SetText(ARENA_TEAM_DISBAND)
                                GameTooltip:Show()
                            </OnEnter>
                            <OnLeave>
                                GameTooltip:Hide()
                            </OnLeave>
                        </Scripts>
                    </Button>
                </Frames>
            </Frame>
            <Frame name="$parentHighlight" hidden="true">
