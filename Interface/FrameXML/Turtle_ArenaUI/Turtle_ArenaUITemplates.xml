                            <Offset>
                                <AbsDimension x="16" y="0" />
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <Texture name="$parentCaptainIcon" file="Interface\GroupFrame\UI-Group-LeaderIcon" hidden="true">
                    <Size>
                        <AbsDimension x="16" y="16" />
