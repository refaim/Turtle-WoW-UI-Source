                        <FontString name="$parentSeasonLoss" inherits="GameFontHighlightSmall" text="0">
                            <Anchors>
                                <Anchor point="LEFT" relativeTo="$parentSeasonSeparator" relativePoint="RIGHT" />
                            </Anchors>
                        </FontString>
                        <FontString name="$parentRankLabel" inherits="GameFontDisableSmall" text="ARENA_TEAM_RANK" justifyH="CENTER">
