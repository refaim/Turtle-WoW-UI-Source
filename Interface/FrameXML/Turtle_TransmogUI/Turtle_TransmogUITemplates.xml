<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Frame name="TransmogFrameLookTemplate" virtual="true" frameStrata="MEDIUM">
		<Size>
			<AbsDimension x="90" y="110" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" />
		</Anchors>
		<Frames>
			<Button name="$parentButton" inherits="UIPanelButtonTemplate">
				<NormalTexture file="Interface\TransmogFrame\item_bg_normal">
					<Size>
						<AbsDimension x="128" y="128" />
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="-20" y="0" />
							</Offset>
						</Anchor>
					</Anchors>
				</NormalTexture>
				<PushedTexture file="Interface\TransmogFrame\item_bg_selected">
					<Size>
						<AbsDimension x="128" y="128" />
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="-20" y="0" />
							</Offset>
						</Anchor>
					</Anchors>
				</PushedTexture>
				<HighlightTexture file="Interface\TransmogFrame\item_bg_highlight" alphaMode="ADD">
					<Size>
						<AbsDimension x="128" y="128" />
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="-20" y="0" />
							</Offset>
						</Anchor>
					</Anchors>
				</HighlightTexture>
				<Size>
					<AbsDimension x="90" y="128" />
				</Size>
				<Anchors>
					<Anchor point="LEFT" />
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture name="$parentRevert" hidden="true" file="Interface\TransmogFrame\revert">
							<Size>
								<AbsDimension x="16" y="16" />
							</Size>
							<Anchors>
								<Anchor point="TOPLEFT">
									<Offset>
										<AbsDimension x="3" y="-9" />
									</Offset>
								</Anchor>
							</Anchors>
						</Texture>
						<Texture name="$parentCheck" hidden="true" file="Interface\TransmogFrame\check">
							<Size>
								<AbsDimension x="16" y="16" />
							</Size>
							<Anchors>
								<Anchor point="TOPRIGHT">
									<Offset>
										<AbsDimension x="-8" y="-9" />
									</Offset>
								</Anchor>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnClick>
						Transmog_Try(this:GetID(), this:GetName())
					</OnClick>
				</Scripts>
			</Button>
			<DressUpModel name="$parentItemModel" scale="4.0">
				<Size>
					<AbsDimension x="80" y="107" />
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="4" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
			</DressUpModel>
		</Frames>
	</Frame>
	<Frame name="TransmogWonItemTemplate" virtual="true">
		<Size>
			<AbsDimension x="512" y="128" />
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="-200" />
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="$parentItemName" inherits="GameFontNormalLarge" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="208" y="-55" />
							</Offset>
						</Anchor>
					</Anchors>
					<Size>
						<AbsDimension x="160" y="35" />
					</Size>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentIcon" inherits="UIPanelButtonTemplate">
				<NormalTexture name="$parentNormalTexture">
					<Size>
						<AbsDimension x="46" y="46" />
					</Size>
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</NormalTexture>
				<PushedTexture inherits="" />
				<DisabledTexture inherits="" />
				<HighlightTexture inherits="" />
				<Backdrop bgFile="Interface\Tooltips\U1I-Tooltip-Background" tile="true" />
				<Size>
					<AbsDimension x="46" y="46" />
				</Size>
				<Anchors>
					<Anchor point="LEFT">
						<Offset>
							<AbsDimension x="148" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
		</Frames>
	</Frame>
	<Button name="TransmogPlayerSlotTemplate" virtual="true" inherits="UIPanelButtonTemplate" topLevel="true">
		<NormalTexture name="$parentNormalTexture">
			<Size>
				<AbsDimension x="26" y="26" />
			</Size>
			<Anchors>
				<Anchor point="TOPLEFT" />
			</Anchors>
		</NormalTexture>
		<DisabledTexture name="$parentDisabledTexture">
			<Size>
				<AbsDimension x="26" y="26" />
			</Size>
			<Anchors>
				<Anchor point="TOPLEFT" />
			</Anchors>
		</DisabledTexture>
		<PushedTexture name="$parentNormalTexture">
			<Size>
				<AbsDimension x="26" y="26" />
			</Size>
			<Anchors>
				<Anchor point="TOPLEFT" />
			</Anchors>
		</PushedTexture>
		<HighlightTexture name="$parentNormalTexture" />
		<Size>
			<AbsDimension x="32" y="32" />
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentItemIcon">
					<Size>
						<AbsDimension x="26" y="26" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-3" y="3" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture name="$parentBorder" file="Interface\TransmogFrame\transmog_border">
					<Size>
						<AbsDimension x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-2" y="3" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentNoEquip" hidden="true" file="Interface\TransmogFrame\no_equip">
					<Size>
						<AbsDimension x="26" y="26" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-3" y="2" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture name="$parentBorderHi" hidden="true" file="Interface\TransmogFrame\border_1">
					<Size>
						<AbsDimension x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-3" y="2" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture name="$parentBorderFull" hidden="true" file="Interface\TransmogFrame\border_full">
					<Size>
						<AbsDimension x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-3" y="2" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture name="$parentBorderSelected" hidden="true" file="Interface\TransmogFrame\border_2">
					<Size>
						<AbsDimension x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-3" y="2" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture name="$parentRevert" hidden="true" file="Interface\TransmogFrame\revert">
					<Size>
						<AbsDimension x="16" y="16" />
					</Size>
					<Anchors>
						<Anchor point="TOPRIGHT">
							<Offset>
								<AbsDimension x="5" y="10" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture name="$parentBorderHighlight" file="Interface\TransmogFrame\border_3">
					<Size>
						<AbsDimension x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="-3" y="2" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Model name="$parentAutoCast" scale="1.3" hidden="true">
				<Size>
					<AbsDimension x="31" y="30" />
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="-2" y="1" />
						</Offset>
					</Anchor>
				</Anchors>
				<Frames>
				</Frames>
			</Model>
			<Model name="$parentSparkle" scale="0.55" inherits="CooldownFrameTemplate" setAllPoints="false">
				<Size x="22" y="22" />
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="0" y="5" />
						</Offset>
					</Anchor>
				</Anchors>
			</Model>
		</Frames>
		<Scripts>
			<OnMouseDown>
				if (arg1 == "RightButton") then
					Transmog_Try(0, this:GetName(), true)
				else
					Transmog_SelectSlot(this:GetID(), this:GetName())
				end
			</OnMouseDown>
		</Scripts>
	</Button>
</Ui>