	<Script file="BuffFrame.lua"/>
	<Button name="BuffButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="30" y="30"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentIcon"/>
				<FontString name="$parentCount" inherits="NumberFontNormal">
					<Anchors>
						<Anchor point="BOTTOMRIGHT">
							<Offset>
								<AbsDimension x="-2" y="2"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="ARTWORK">
				<FontString name="$parentDuration" inherits="GameFontNormalSmall">
					<Anchors>
						<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$parent"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				this.buffFilter = "HELPFUL|HARMFUL";
				BuffButton_OnLoad();
			</OnLoad>
			<OnEvent>
				BuffButton_OnEvent(event);
			</OnEvent>
			<OnUpdate>
				BuffButton_OnUpdate();
			</OnUpdate>
			<OnClick>
				BuffButton_OnClick();
			</OnClick>
			<OnEnter>
				GameTooltip:SetOwner(this, "ANCHOR_BOTTOMLEFT");
				GameTooltip:SetPlayerBuff(this.buffIndex);
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Button>
	<Button name="BuffButtonHelpful" inherits="BuffButtonTemplate" virtual="true">
